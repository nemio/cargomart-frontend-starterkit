[ignore]
.*/node_modules/config-chain/test/broken.json
.*/node_modules/immutable/dist/immutable.js.flow

[include]

[libs]
./interfaces/global.js
./flow-typed
[lints]
untyped-type-import=error

[options]
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/interfaces/CSSFlowStub.js'
module.name_mapper='^api\(.*\)$' -> '<PROJECT_ROOT>/src/api\1'
module.name_mapper='^assets\(.*\)$' -> '<PROJECT_ROOT>/src/assets\1'
module.name_mapper='^components\(.*\)$' -> '<PROJECT_ROOT>/src/components\1'
module.name_mapper='^containers\(.*\)$' -> '<PROJECT_ROOT>/src/containers\1'
module.name_mapper='^helpers\(.*\)$' -> '<PROJECT_ROOT>/src/helpers\1'
module.name_mapper='^middlewares\(.*\)$' -> '<PROJECT_ROOT>/src/middlewares\1'
module.name_mapper='^store\(.*\)$' -> '<PROJECT_ROOT>/src/store\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/utils\1'
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
esproposal.export_star_as=enable

[strict]
